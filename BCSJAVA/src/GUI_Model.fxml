<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="LogicClass">
   <children>
      <Pane prefHeight="254.0" prefWidth="260.0">
         <children>
            <TextField fx:id="displayField" alignment="CENTER_RIGHT" editable="false" layoutX="44.0" layoutY="33.0">
               <font>
                  <Font name="Apple Braille Pinpoint 6 Dot" size="14.0" />
               </font>
            </TextField>
            <Label fx:id="mlabel" layoutX="96.0" layoutY="74.0" text="Label" />
            <Button fx:id="mClear" layoutX="151.0" layoutY="127.0" mnemonicParsing="false" onAction="#clearTextField" text="Clear" />
            <Button fx:id="mSubmit" layoutX="24.0" layoutY="127.0" mnemonicParsing="false" onAction="#submit" text="Submit" />
         </children>
      </Pane>
   </children>
</AnchorPane>
